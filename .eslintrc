{
  "extends": ["byurhan"],
  "rules": {
    "new-cap": 0,
    "no-plusplus": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "lf",
        "semi": false,
        "tabWidth": 2,
        "printWidth": 80,
        "singleQuote": true,
        "trailingComma": "all",
        "jsxBracketSameLine": false
      }
    ]
  }
}
